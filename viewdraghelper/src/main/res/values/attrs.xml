<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwipeRecyclerViewItem">
        <attr name="srvi_swipeDirection" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="srvi_bottomMode" format="enum">
            <enum name="pullOut" value="0" />
            <enum name="layDown" value="1" />
        </attr>
        <attr name="srvi_springDistance" format="dimension" />
    </declare-styleable>
</resources>