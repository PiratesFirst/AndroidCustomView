<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.bingoogolapple.acvp.recyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.HelloworldActivity"
            android:label="HelloWorldDemo" />
        <activity
            android:name=".activity.Helloworld2Activity"
            android:label="HelloWorld2Demo" />
        <activity
            android:name=".activity.HeadindexActivity"
            android:label="HeadindexDemo" />
        <activity
            android:name=".activity.DownloadActivity"
            android:label="DownloadDemo" />
        <activity
            android:name=".activity.DecorationActivity"
            android:label="DecorationDemo" />
        <activity
            android:name=".activity.SwipeActivity"
            android:label="SwipeDemo" />
    </application>

</manifest>
